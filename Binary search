def nadji(niz,element):
	prvi=0
	poslednji=len(niz)-1
	
	while prvi<=poslednji:
		sredina=int((prvi+poslednji)/2)
		if niz[sredina]==element:
			return sredina
		elif niz[sredina]>element:
			poslednji=sredina-1
		else:
			prvi=sredina+1 
	
	return False
	"""
